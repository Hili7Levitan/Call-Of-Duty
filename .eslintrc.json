{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [2, {"js" : 2}],
        "no-param-reassign": "warn",
        "no-underscore-dangle": "off",
        "max-len": "off"
    }
}